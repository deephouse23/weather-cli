{
  "name": "weather-cli-16bit",
  "version": "0.3.21",
  "description": "A simple, intelligent weather CLI with smart location detection - no quotes needed",
  "main": "index.js",
  "type": "module",
  "bin": {
    "weather": "./bin/weather.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "files": [
    "bin/",
    "src/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "start": "node bin/weather.js",
    "dev": "node --watch bin/weather.js",
    "test": "node test-modules.js",
    "lint": "echo 'No linting configured'",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "weather",
    "cli",
    "command-line",
    "forecast",
    "temperature",
    "no-quotes",
    "smart-location",
    "openweathermap",
    "location-search",
    "secure-auth",
    "responsive-design",
    "weather-forecast"
  ],
  "author": "16bitweather <justinelrod111@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deephouse23/weather-cli.git"
  },
  "homepage": "https://16bitweather.co",
  "bugs": {
    "url": "https://github.com/deephouse23/weather-cli/issues"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "axios-retry": "^4.0.0",
    "boxen": "^7.1.1",
    "chalk": "^5.5.0",
    "commander": "^12.1.0",
    "dotenv": "^16.6.1",
    "inquirer": "^12.9.0",
    "keytar": "^7.9.0",
    "ora": "^8.2.0"
  }
}
